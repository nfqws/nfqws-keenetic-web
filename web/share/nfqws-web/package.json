    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-router": "^1.157.18",
    "@uiw/codemirror-theme-vscode": "^4.25.4",
    "@uiw/react-codemirror": "^4.25.4",
    "clsx": "^2.1.1",
    "react": "^19.2.4",
